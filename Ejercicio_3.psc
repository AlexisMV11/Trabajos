Proceso Ejercicio_3
	Definir klm, gasr, consMetro, gasklm Como Real;
	klm <- 1.5;
	gasr <- 40;
	consMetro <- gasr/(klm*1000);
	Escribir 'Consumo de gasolina por metro: ', consMetro, ' litros';
	gasklm <- consMetro*2000;
	Escribir 'Gasolina necesaria para recorrer 2000 metros: ', gasklm, ' litros';
FinProceso
