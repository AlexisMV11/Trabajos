Proceso sin_titulo
	
	Escribir "Hola, ¿Me puedes decir tu primer nombre?";
	Definir Nom Como Caracter;
	Leer Nom;
	Escribir "Puedes decirme dos numeros enteros ", Nom;
	Definir N1, N2, SumaFin Como Entero;
	Leer N1, N2;
	Escribir "Ahora los sumaremos";
	SumaFin <- N1 + N2;
	Escribir N1, "+", N2, "=", SumaFin;
	
FinProceso
